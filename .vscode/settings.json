{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll": true,
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
  ],
  "editor.formatOnSave": false,
  "eslint.alwaysShowStatus": true,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "cSpell.words": [
    "GIPHY",
    "Gifs",
    "Typers",
    "apos",
    "typer",
    "typers"
  ]
}